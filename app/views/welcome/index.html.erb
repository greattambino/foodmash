<h1>Top 20 Trending Restaurants in SF!</h1>
<% time = Time.now %>
<h3><%= time.httpdate %> </h3>


<% @jresp['businesses'].each_with_index do |business, i| %>
    <%= (i + 1) %>.  Name: <%= business['name'] %><br>
      &nbsp;&nbsp;&nbsp;&nbsp; -Phone: <%= business['phone'] %><br>
      &nbsp;&nbsp;&nbsp;&nbsp; -Review Count: <%= business['review_count'] %><br>
      &nbsp;&nbsp;&nbsp;&nbsp; -Rating: <%= business['rating'] %><br>
      <br>
<% end %>
